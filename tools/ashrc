alias mongo=$'mongo "${TIDEPOOL_STORE_SCHEME}://${TIDEPOOL_STORE_ADDRESSES}/${TIDEPOOL_STORE_DATABASE}$([ -n "$TIDEPOOL_STORE_OPT_PARAMS" ] && echo ?)$(echo $TIDEPOOL_STORE_OPT_PARAMS|awk $\'BEGIN { RS="&"} $0 ~ /replicaSet/\')" $( [ "$TIDEPOOL_STORE_TLS" == "true" ] && echo "--ssl" ) --authenticationDatabase $(echo $TIDEPOOL_STORE_OPT_PARAMS|awk \'BEGIN { RS="&"; FS="="} $0 ~ /authSource/ {print $2}\') --username $TIDEPOOL_STORE_USERNAME $([ -n "$TIDEPOOL_STORE_PASSWORD" ] && echo --password) $TIDEPOOL_STORE_PASSWORD'
